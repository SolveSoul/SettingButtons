<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SettingButton">
        <attr name="sbTitleText" format="string" />
        <attr name="sbDescriptionText" format="string" />
        <attr name="sbTitleForeground" format="color" />
        <attr name="sbDescriptionForeground" format="color" />
        <attr name="sbBorders" format="enum">
            <enum name="none" value="0" />
            <enum name="bottom" value="1" />
        </attr>
    </declare-styleable>

</resources>